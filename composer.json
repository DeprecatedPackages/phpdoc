{
  "name": "contributte/phpdoc",
  "description": "Doctrine Annotations for Nette Framework",
  "keywords": [
    "nette",
    "annotations",
    "reflection",
    "phpdoc"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/phpdoc",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "nette/reflection": "~2.4.2",
    "doctrine/annotations": "^1.6.0",
    "doctrine/cache": "^1.7.1"
  },
  "require-dev": {
    "nette/di": "~2.4.13",
    "ninjify/nunjuck": "^0.2.0",
    "ninjify/qa": "^0.8.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "suggest": {
    "nette/di": "to register Nette extensions [PhpDocExtension]"
  },
  "conflict": {
    "nette/di": "<2.4.13",
    "nette/utils": "<2.5.2"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\PhpDoc\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
